<%- include('../partials/header') %>

    <body>
        <main>
            <h1>Web Dev Skills</h1>
            <nav>
                <ul>
                    <li><a href="/">&lt; Go Back</a></li>
                </ul>
            </nav>

            <div>
                <ul>
                    <% skills.forEach(function(s) { %>
                        <li>
                            <a href="/skills/<%= s.id %>">
                                <%= s.name %>
                                    <%= s.skill %>
                            </a>
                        </li>
                        <% }); %>
                </ul>
            </div>
            <a href="/skills/new">Add New Skill</a>
        </main>

        <%- include('../partials/footer') %>